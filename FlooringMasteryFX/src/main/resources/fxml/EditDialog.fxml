                        <Insets right="20.0" />
                    </GridPane.margin>
                </Text>
                <Text layoutX="10.0" layoutY="91.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Product" GridPane.halignment="RIGHT" GridPane.rowIndex="3">
